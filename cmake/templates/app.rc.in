APP_ICON ICON "@ICON_PATH@"

1 VERSIONINFO
FILEVERSION     @PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@, @PROJECT_VERSION_PATCH@, 0
PRODUCTVERSION  @PROJECT_VERSION_MAJOR@, @PROJECT_VERSION_MINOR@, @PROJECT_VERSION_PATCH@, 0
BEGIN
  BLOCK "StringFileInfo"
  BEGIN
    BLOCK "040904E4"
    BEGIN
        VALUE "CompanyName", "Home"
        VALUE "FileDescription", "@PROJECT_DESCRIPTION@"
        VALUE "FileVersion", "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@"
        VALUE "InternalName", "@PROJECT_NAME@"
        VALUE "LegalCopyright", "(c) Home. All rights reserved."
        VALUE "OriginalFilename", "@PROJECT_NAME@.exe"
        VALUE "ProductName", "@APP_NAME@"
        VALUE "ProductVersion", "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@"
    END
  END
  BLOCK "VarFileInfo"
  BEGIN
    VALUE "Translation", 0x0, 1252
  END
END